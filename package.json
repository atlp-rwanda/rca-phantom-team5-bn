{
	"name": "phantom-backend",
	"version": "1.0.0",
	"description": "Elevate the public transport experience",
	"main": "src/index.ts",
	"scripts": {
		"deleteOnetable": "npx sequelize db:migrate:undo --name 20230311000000-create-users.js",
		"createOnetable": "npx sequelize-cli db:migrate --to 20230311000000-create-users.js",
		"deleteAllTables": "tsc && npx sequelize db:migrate:undo:all",
		"createAllTables": "tsc && npx sequelize db:migrate",
		"deleteAllSeeds": "tsc && npx sequelize db:seed:undo:all",
		"createAllSeeds": "tsc && npx sequelize db:seed:all",
		"start": "tsc && node dist/index.js",
		"dev": "tsx watch src/index.ts",
		"test": "tsc && nyc mocha",
		"lint": "eslint src --fix"
	},
	"mocha": {
		"extension": [
			"ts"
		],
		"spec": "src/test/**/*.spec.ts",
		"require": "ts-node/register"
	},
	"nyc": {
		"extends": "@istanbuljs/nyc-config-typescript",
		"reporter": [
			"text-summary",
			"lcov"
		],
		"all": "true",
		"exclude": [
			"dist",
			"coverage"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Stackup-Rwanda/phantom-backend.git"
	},
	"keywords": [
		"buses",
		"transport",
		"typescript"
	],
	"author": "ceelogre",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Stackup-Rwanda/phantom-backend/issues"
	},
	"homepage": "https://github.com/Stackup-Rwanda/phantom-backend#readme",
	"dependencies": {
		"@babel/register": "^7.21.0",
		"dotenv": "^16.0.3",
		"express": "^4.17.1",
		"morgan": "^1.10.0",
		"pg": "^8.10.0",
		"pg-hstore": "^2.3.4",
		"reflect-metadata": "^0.1.13",
		"sequelize": "^6.29.2",
		"sequelize-cli": "^6.6.0",
		"sequelize-typescript": "^2.1.5",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^4.6.2"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^1.0.1",
		"@types/chai": "^4.3.4",
		"@types/express": "^4.17.17",
		"@types/mocha": "^8.0.3",
		"@types/morgan": "^1.9.4",
		"@types/node": "^13.13.52",
		"@types/swagger-jsdoc": "^6.0.1",
		"@types/swagger-ui-express": "^4.1.3",
		"@types/validator": "^13.7.13",
		"@typescript-eslint/eslint-plugin": "^4.0.1",
		"@typescript-eslint/parser": "^4.0.1",
		"assert": "^2.0.0",
		"chai": "^4.3.7",
		"eslint": "^7.8.1",
		"mocha": "^10.2.0",
		"nyc": "^15.1.0",
		"source-map-support": "^0.5.19",
		"ts-node": "^9.0.0",
		"ts-node-dev": "^2.0.0",
		"tsx": "^3.12.5",
		"typescript": "^4.9.5"
	}
}
